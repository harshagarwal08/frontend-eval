{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "commonjs": true
    },
    "extends": [ "airbnb", "plugin:prettier/recommended" ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
            "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]}
    ]}
}
